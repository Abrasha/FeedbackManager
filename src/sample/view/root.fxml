<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.view.RootController">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="Add...">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#addCityAction" text="City" />
                  <MenuItem mnemonicParsing="false" onAction="#addRestaurantAction" text="Restaurant" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Remove..">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#deleteCityAction" text="City" />
                  <MenuItem mnemonicParsing="false" onAction="#deleteRestaurantAction" text="Restaurant" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <left>
      <VBox maxWidth="-Infinity" prefHeight="450.0" prefWidth="250.0" spacing="10.0">
         <children>
            <ComboBox fx:id="cbCities" maxWidth="1.7976931348623157E308">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </ComboBox>
            <ListView fx:id="lvRestaurants" BorderPane.alignment="CENTER" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </ListView>
         </children>
      </VBox>
   </left>
   <center>
      <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
         <children>
            <Separator orientation="VERTICAL" prefHeight="200.0">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin>
            </Separator>
            <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
               <children>
                  <StackPane fx:id="spContent" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="150.0" prefWidth="200.0" style="-fx-border-color: red;" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
         </children>
      </HBox>
   </center>
</BorderPane>
